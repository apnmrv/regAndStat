{% extends 'base.html.twig' %}

{% block title %}Last days visitors{% endblock %}


{% block body %}

<div class="example-wrapper">
    <h1>Last days visitors</h1>
    <ul>
    {% for elem in data %}
        <li>User : {{ elem.name }} </li>
        <p>Visits</p>
        <ul>
            {% for k,v in elem.visits %}
            <li>Date : {{ k }} || Visits : {{ v }}</li>
            {% endfor %}
        </ul>
        </br>
    {% endfor %}
    </ul>

</div>
{% endblock %}
